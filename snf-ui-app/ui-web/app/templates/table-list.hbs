<thead>
    <tr>
        {{#with sortProperties as sort}}
            {{#each ths}}
                {{table-th label=label field=field cls=cls sort=sort action="sortBy"}}
            {{/each}}
        {{/with}}
    </tr>
</thead>
<tbody>
{{#each sortedModel itemController=itemController}}
    {{#if object_view}}{{view "object"}}{{/if}}
    {{#if container_view}}{{view "containers-list-item"}}{{/if}}
{{/each}}
</tbody>
