{{outlet}}
<h4>Container:{{id}}</h4>
<ul>
{{#each}}
    <li>
    {{#if is_dir}}
        {{#link-to 'container' name }}
            {{fa-icon "fa-folder-open"}}
            {{stripped_name}}
        {{/link-to}}
    {{else}}
        {{fa-icon "fa-file-o"}}
        {{stripped_name}}
        {{view_src}}
        <img {{bind-attr scr=view_src}} />
    {{/if}}
    </li>
{{/each}}
</ul>
      
