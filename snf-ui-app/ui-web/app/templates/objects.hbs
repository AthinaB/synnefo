{{outlet}}
<h4>Objects</h4>
<ul>
{{#each}}
    <li>
    {{#if is_dir}}
        {{#link-to 'objects' name }}
            {{fa-icon "fa-folder-open"}}
            {{stripped_name}}
        {{/link-to}}
    {{else}}
        {{fa-icon "fa-file-o"}}
        {{stripped_name}}
        <img {{bind-attr scr=view_src}} />
        <a {{action 'deleteObject'}} class="button tiny alert">Delete object</a>
    {{/if}}
    </li>
{{/each}}
</ul>
      
