{{#if listView}}
    <li>
        <div class="wrap clearfix">
            {{view "title-sortable" field="name" classNames="name-col"}}
            <div class="col select-col">Project</div>
            {{view "title-sortable" field="bytes" title="Size" classNames="data-col"}}
            {{view "title-sortable" field="last_modified" title="modified" classNames="date-col"}}
            {{view "title-sortable" field="count" title="items" classNames="data-col"}}
            <div class="col actions-col"></div>
        </div>
    </li>
{{/if}}
{{#each container in sortedModel itemController="container" itemView="container"}}{{/each}}
