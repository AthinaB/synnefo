#!/usr/bin/make -f

%:
	dh $@ --with python2

package=snf-common

override_dh_auto_install:
	dh_auto_install
	
	# this package provides the synnefo namespace __init__.py
	for py in $(shell pyversions -vr debian/control); do \
		cp synnefo/__init__.py debian/$(package)/usr/lib/python$$py/*-packages/synnefo/; \
	done
